page{
  background-color:#eeeff0;
}
.card {
  background-color: #fff;
  margin-top: 20rpx;
}
.content:last-child{
  margin-bottom: 20rpx;
}
.card-header {
  position: relative;
  display: flex;
  justify-content: space-between;
  padding: 0 20rpx;
  line-height: 82rpx;
}

.card-header::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(.5);
  transform: scale(.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border: 0 solid #e5e5e5;
  border-bottom-width: 1px;
  border-radius: 4px;
}

.card-header-left {
  font-size: 30rpx;
  color: rgb(51, 51, 51);
}

.card-header-right {
  font-size: 28rpx;
  color: rgb(102, 102, 102);
}

.card-body {
  padding: 22rpx 20rpx;
}

.card-body-content-left {
  display: flex;
  flex-direction: column;
}

.card-body-content__name {
  font-size: 30rpx;
  color: rgb(51, 51, 51);
}

.card-body-content__address {
  margin-top: 8rpx;
  margin-bottom: 25rpx;
  font-size: 24rpx;
  color: rgb(102, 102, 102);
}

.card-body-content .card-body-content__description {
  color: rgb(153, 153, 153);
}

.icon {
  width: 46rpx;
  height: 46rpx;
  background-size: cover;
}

.icon-map {
  background-image: url(data:image/jpeg;base64,);
}

.card-body-content {
  display: flex;
  justify-content: space-between;
}

.card-body-content-map {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 10rpx;
  padding-right: 10rpx;
}

.card-body-content-map__text {
  font-size: 22rpx;
  color: rgb(102, 102, 102);
}

.card-body-price {
  text-align: right;
}

.card-body-price__label {
  font-size: 28rpx;
  color: rgb(51, 51, 51);
}

.card-body-price__prefix {
  font-size: 30rpx;
  color: #059983;
}

.card-body-price__text {
  font-size: 32rpx;
  color: #059983;
}

.card-footer {
  position: relative;
  display: flex;
  justify-content: flex-end;
  padding: 18rpx 20rpx;
}

.card-footer::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(.5);
  transform: scale(.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border: 0 solid #e5e5e5;
  border-top-width: 1px;
  border-radius: 4px;
}

[class^="card-footer-btn"] {
  border-radius: 6rpx;
  margin-left: 20rpx;
  height: 58rpx;
  font-size: 26rpx;
  line-height: 58rpx;
  text-align: center;
  color: rgb(102, 102, 102);
}

.card-footer-btn__qr {
  width: 156rpx;
  color: #fff;
  background-color: #059983;
}

.card-footer-btn__refund {
  position: relative;
  width: 135rpx;
  font-size: 26rpx;
}

.card-footer-btn__refund::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(.5);
  transform: scale(.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border: 0 solid #e5e5e5;
  border-width: 1px;
  border-radius: 4px;
}

.card-footer-btn__refunded {
  position: relative;
  width: 130rpx;
}

.card-footer-btn__refunded::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(.5);
  transform: scale(.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border: 0 solid #e5e5e5;
  border-width: 1px;
  border-radius: 4px;
}

.refundActionsheet-subname {
  padding-top: 8rpx;
  font-size: 26rpx !important;
  line-height: 0.8 !important;
}

.failed {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.failed__img {
  margin: 90rpx auto 50rpx;
  width: 126rpx;
  height: 168rpx;
}
.failed__text {
  margin-bottom: 40rpx;
  font-size: 28rpx;
  color: #333;
}

.failed .failed__btn {
  width: 156rpx;
  height: 58rpx;
  line-height: 58rpx;
  font-size: 26rpx;
  color: #fff !important;
  background-color: #059983 !important;
}
.qrcode{
  width: 300rpx;
  height: 300rpx;
  display: block;
  margin: 0 auto;
}