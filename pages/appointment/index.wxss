page {
  background-color: #eeeff0;
}

.title {
  padding: 0 20rpx;
  height: 75rpx;
  line-height: 75rpx;
  font-size: 26rpx;
  color: rgb(153, 153, 153);
}

.radio-slot {
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex: 1;
}

.icon {
  width: 46rpx;
  height: 46rpx;
  background-size: cover;
}

.icon-map {
  background-image: url(data:image/jpeg;base64,);
}

.radio-slot-map {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 10rpx;
  padding-right: 10rpx;
}

.radio-slot-map__text {
  font-size: 22rpx;
  color: rgb(102, 102, 102);
}

.radio-slot__name {
  font-size: 30rpx;
  color: rgb(51, 51, 51);
}

.radio-slot__distance {
  margin-left: 10rpx;
  font-size: 22rpx;
  color: rgb(153, 153, 153);
}

.radio-slot-left {
  padding-top: 10rpx;
  padding-left: 10rpx;
}

.radio-slot-left-row {
  margin-bottom: 10rpx;
  display: flex;
  align-items: center;
}

.icon-location {
  margin-top: -4rpx;
  margin-right: 10rpx;
  width: 22rpx;
  height: 32rpx;
  background-size: cover;
  background-image: url(data:image/png;base64,);
}

.radio-slot-location {
  font-size: 24rpx;
  color: rgb(102, 102, 102);
}

.time-picker {
  height: 229rpx;
  display: flex;
  flex-wrap: wrap;
  align-content: space-between;
  padding: 29rpx 20rpx;
  background-color: #fff;
  justify-content: space-between;
}

.time-picker__item {
  position: relative;
  width: 210rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 30rpx;
  color: rgb(153, 153, 153);
}

.time-picker__item--active {
  background-color: #059983;
  color: #fff;
  border-radius: 4rpx;
}

.time-picker__item::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(.5);
  transform: scale(.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border: 0 solid #e5e5e5;
  border-width: 1px;
  border-radius: 8rpx;
}

.time-picker__item--active::after {
  display: none;
}

.footer {
  margin-top: 53rpx;
}

.footer .footer-btn {
  color: #fff !important;
  background-color: #059983 !important;
}