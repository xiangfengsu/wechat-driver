page{
  background-color:#eeeff0;
}

.info {
  background-color: #fff;
}

.info-body {
  position: relative;
  display: flex;
  justify-content: space-between;
  padding: 32rpx 20rpx;
}

.info-body::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(.5);
  transform: scale(.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border: 0 solid #e5e5e5;
  border-width: 1px 0px;
  border-radius: 4px
}

.info-body-left {
  display: flex;
  flex-direction: column;
}

.info-body-left__title {
  font-size: 30rpx;
  color: rgb(51, 51, 51);
  margin-bottom: 8rpx;
}

.info-body-left__description {
  font-size: 24rpx;
}

.icon {
  width: 46rpx;
  height: 46rpx;
  background-size: cover;
}

.icon-map {
  background-image: url(data:image/jpeg;base64,);
}

.info-body-right-map {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 10rpx;
  padding-right: 10rpx;
}

.info-body-right-map__text {
  font-size: 22rpx;
  color: rgb(102, 102, 102);
}

.info-footer {
  position: relative;
  display: flex;
  align-items: center;
  padding: 30rpx 26rpx 30rpx;
}

.info-footer::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(.5);
  transform: scale(.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border: 0 solid #e5e5e5;
  border-bottom-width: 1px;
  border-radius: 4px
}

.icon-time {
  margin-right: 22rpx;
  width: 32rpx;
  height: 32rpx;
  background-image: url(data:image/png;base64,);
}

.info-footer__text {
  font-size: 24rpx;
  color: rgb(102, 102, 102);
}

.product {
  margin-top: 20rpx;
  background-color: #fff;
}

.product .product-card {
  padding-top: 35rpx;
  padding-bottom: 44rpx;
}

.product-coupons {
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 20rpx;
  padding: 20rpx 0;
}

.product-coupons::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(.5);
  transform: scale(.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border: 0 solid #e5e5e5;
  border-bottom-width: 1px;
}

.product-coupons-left {
  display: flex;
  flex-direction: column;
}

.product-coupons-left__title {
  font-size: 24rpx;
  color: rgb(153, 153, 153);
}

.product-coupons-left__price {
  margin-top: 12rpx;
  font-size: 30rpx;
  color: rgb(255, 0, 0);
}

.icon-pay {
  width: 38rpx;
  height: 40rpx;
  background-size: contain;
  background-repeat: no-repeat;
  background-image: url(data:image/jpeg;base64,);
}

.icon-pay--active {
  position: relative;
  top: 3rpx;
  width: 38rpx;
  height: 40rpx;
  background-image: url(data:image/jpeg;base64,);
}

.product-total {
  padding-top: 22rpx;
  padding-bottom: 20rpx;
  padding-right: 20rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.product-total__prefix {
  font-size: 28rpx;
}

.product-total__symbol {
  font-size: 30rpx;
  color: #059983;
}

.product-total__price {
  font-size: 32rpx;
  color: #059983;
}

.pay-way {
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
  padding: 0 20rpx;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
  font-size: 26rpx;
  color: rgb(102, 102, 102);
}

.pay {
  padding: 58rpx 0 20rpx;
}

.pay .pay-btn {
  color: #fff !important;
  background-color: #059983 !important;
}

.pay-btn__text {
  font-size: 34rpx;
}

.pay-btn__symbol {
  margin-left: 10rpx;
  font-size: 30rpx;
}

.pay-btn__price {
  margin-left: 10rpx;
  font-size: 32rpx;
}

.color-green {
  color: #059983;
}