page {
  background-color: #eeeff0;
}

.header {
  position: relative;
}

.header-img {
  width: 100%;
  height: 300rpx;
  vertical-align: middle;
}

.header-btn {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 60rpx;
  margin: 0 auto;
  width: 247rpx;
  height: 68rpx;
}

.icon {
  width: 38rpx;
  height: 38rpx;
  background-size: cover;
}

.icon-lunch {
  background-image: url(data:image/png;base64,);
}

.icon-dinner {
  background-image: url(data:image/png;base64,);
}

.x-card__title {
  display: flex;
  align-items: center;
  padding: 5rpx 0 15rpx;
}

.x-card__title-text {
  margin-left: 10rpx;
  font-size: 26rpx;
  color: rgb(155, 155, 155);
}

.products {
  margin-top: 20rpx;
}

.products-card {
  margin-top: 1px;
}

.dialog-sign {
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
  padding-bottom: 10rpx;
}

.dialog-sign__img {
  width: 120rpx;
  height: 120rpx;
  margin-bottom: 18rpx;
}

.dialog-sign__text {
  font-size: 38rpx;
  margin-bottom: 18rpx;
}

.dialog-sign__message {
  font-size: 28rpx;
  color: rgb(155, 155, 155);
}